        <addForeignKeyConstraint baseColumnNames="userrole_id"
                                 baseTableName="rel_city_service__userrole"
                                 constraintName="fk_rel_city_service__userrole__userrole_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_role"/>
    </changeSet>
</databaseChangeLog>
